{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["airbnb"],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [0],
      "react/prop-types": [0],
      "react/destructuring-assignment": [1],
      "no-console": [0],
      "semi": [2, "always"]
    }
}
  