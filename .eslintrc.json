{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": [0],
      "react/prop-types": [0],
      "react/destructuring-assignment": ["never"],
      "no-console": [0],
      "semi": [2, "always"]
    }
}
  